{
  "name": "andcards-icons-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "storybook-web-build": "cd ./storybooks/web && npm run build-storybook",
    "storybook-web-watch": "cd ./storybooks/web && npm run storybook-web-watch",
    "storybook-web": "cd ./storybooks/web && npm run storybook",
    "storybook-native-watch": "cd ./storybooks/native && npm run storybook-native-watch",
    "storybook-ios": "cd ./storybooks/native && npm run ios",
    "storybook-android": "cd ./storybooks/native && npm run android",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js --ext .jsx src",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "npm run lint && npm run clean && babel src --out-dir lib && npm test && cp -f LICENSE ./lib/LICENSE && cp -f ./package-lib.json ./lib/package.json",
    "release": "npm run build && npm run storybook-web-build",
    "publish": "npm run release && cd ./lib && npm publish"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.6.0",
    "react-native": "^0.57.4",
    "svgs": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.0.4",
    "prettier": "^1.14.3",
    "prop-types": "^15.5.10",
    "react": "^16.6.2",
    "react-native": "^0.57.4",
    "rimraf": "^2.3.4",
    "svgs": "^3.3.1"
  }
}
